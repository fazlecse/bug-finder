.bottom-nav {
   width: 100%;
   background: var(--white);
   box-shadow: var(--shadow);
   z-index: 1046;
   display: flex;
   align-items: center;
   justify-content: space-around;
   .link-item {
      text-align: center;
      padding: 5px;
      top: 0;
      transition: 0.4s;
      position: relative;
      i {
         color: var(--primary);
         font-size: 16px;
         width: 40px;
         height: 40px;
         border-radius: 40px;
         display: flex;
         align-items: center;
         justify-content: center;
         transition: 0.4s;
         position: relative;
         top: 0;
         margin: auto;
         margin-bottom: 5px;
      }
      span {
         font-size: 14px;
         display: block;
         color: var(--black);
      }
      &:hover,
      &.active {
         i {
            background: var(--primary);
            color: var(--white);
            // top: -15px;
         }
      }
   }
   .navbar-toggler {
      padding: 0;
      &:focus {
         box-shadow: 0 0 0 0rem !important;
      }
   }
}
.user-side-menu {
   .menu-links {
      list-style: none;
      margin-bottom: 0;
      padding-left: 0;
      overflow-y: auto;
      li {
         margin-bottom: 5px;
         a {
            position: relative;
            color: var(--fontColor);
            text-transform: capitalize;
            font-weight: 400;
            width: 100%;
            display: block;
            border-radius: 7px;
            padding: 8px 15px 8px 15px;
