.btn,
.btn-custom {
   position: relative;
   background: var(--primary);
   border: none;
   font-family: "Oswald", sans-serif;
   text-transform: uppercase;
   display: inline-block;
   color: var(--white);
   text-align: center;
   font-size: 16px;
   font-weight: 400;
   padding: 25px 55px;
   line-height: 1;
   border-radius: 15px;
   overflow: hidden;
   z-index: 1;
   transition: 0.4s;
   i {
      margin-right: 5px;
   }
   &::before {
      content: "";
      position: absolute;
      width: 0;
      height: 50%;
      top: 0;
      left: 0;
      background: var(--primary);
      border-top-right-radius: 4px;
      border-top-left-radius: 4px;
      z-index: -1;
      transition: 0.4s;
   }
   &::after {
      content: "";
      position: absolute;
      width: 0;
      height: 50%;
      bottom: 0;
      right: 0;
      background: var(--primary);
      border-bottom-right-radius: 4px;
      border-bottom-left-radius: 4px;
      z-index: -1;
      transition: 0.4s;
   }
}
.btn-custom:hover {
   &::before,
   &::after {
      width: 100%;
   }
}
a.btn-custom {
   color: var(--white);
}
