// changelog section
.changelog-section {
   padding: 100px 0;
   .changelog-box {
      position: relative;
      padding-bottom: 50px;
      &::before {
         content: "";
         width: 2px;
         height: 100%;
         background: var(--bgLight);
         position: absolute;
         top: 0;
         left: 14px;
      }
      .date {
         position: relative;
         padding-left: 45px;
         margin-bottom: 30px;
         display: flex;
         justify-content: space-between;
         &::before {
            content: "";
            width: 30px;
            height: 30px;
            border-radius: 20px;
            background: var(--primary);
            border: 5px solid var(--white);
            box-shadow: 0px 0px 5px var(--primary);
            position: absolute;
            top: 0;
            left: 0;
         }
         a {
            color: var(--black);
            text-decoration: underline;
            font-size: 16px;
            transition: 0.4s;
            &:hover {
               color: var(--primary);
            }
         }
      }
      .info {
         background: var(--white);
         box-shadow: var(--shadow);
         border-radius: 5px;
         padding: 20px;
         margin-left: 45px;
         li {
            margin-bottom: 10px;
            color: var(--fontColor);
            &:last-child {
               margin-bottom: 0;
            }
            a {
               color: var(--fontColor);
            }
            a:hover {
               color: var(--primary);
            }
         }
      }
   }
}
.rtl .changelog-section {
   .changelog-box {
      &::before {
         left: auto;
         right: 14px;
      }
      .date {
         padding-left: 0;
         padding-right: 45px;
         &::before {
            left: auto;
            right: 0;
         }
      }
      .info {
         margin-left: 0;
         margin-right: 45px;
         ul {
            padding-left: 0;
            padding-right: 1rem;
         }
      }
   }
}

// cart section
.cart-section {
   padding: 100px 0;
   .table-wrapper {
      // box-shadow: var(--shadow);
      border: 1px solid var(--borderColor);
      border-radius: 5px;
      .table {
         color: var(--gray);
         margin-bottom: 0;
         tbody > tr:nth-of-type(even) > * {
            --bs-table-accent-bg: var(--white) !important;
         }
         tbody > tr:nth-of-type(odd) > * {
            --bs-table-accent-bg: var(--white);
         }
         thead {
            background: var(--borderColor);
            color: var(--black);
            text-transform: capitalize;
            tr {
               border: none;
            }
         }
         tbody {
            border: none;
         }
         tr {
            border: none;
            border-top: 1px solid var(--borderColor);
            th {
               font-weight: 700;
               border: none !important;
               padding: 15px;
            }
            td {
               padding: 15px;
               color: var(--gray);
               border: none !important;
               text-transform: capitalize;
               margin: 5px !important;
               vertical-align: middle;
               img {
                  min-width: 80px;
                  max-width: 80px;
                  height: 80px;
                  border-radius: 5px;
                  display: block;
                  margin-bottom: 10px;
               }
               .action-btn {
                  background: var(--white);
                  border: none;
                  height: 40px;
                  width: 40px;
                  border-radius: 50px;
                  padding-top: 2px;
                  text-align: center;
                  color: var(--secondary);
                  i {
                     font-size: 16px;
                  }
               }
               .form-check-input {
                  border-color: var(--border);
                  cursor: pointer;
                  &:focus {
                     box-shadow: 0 0 0 0rem rgba(13, 110, 253, 0.25);
                  }
               }
               .form-check-input:checked {
                  background-color: var(--secondary);
                  border-color: var(--secondary);
               }
            }

            .quantity {
               display: flex;
               align-items: center;
               border-radius: 5px;
               width: 120px;
               overflow: hidden;
               .btn-inc-dec {
                  width: 40px;
                  height: 40px;
                  border-radius: 0px;
                  background: var(--borderColor);
                  color: var(--fontColor);
                  font-size: 18px;
               }
               .form-control {
                  width: 40px;
                  height: 40px;
                  margin: 0 0px;
                  background: var(--borderColor);
                  border-radius: 0px;
                  border: none;
                  color: var(--fontColor);
                  text-align: center;
                  &::placeholder {
                     color: var(--borderColor);
                  }
                  &:focus {
                     box-shadow: 0 0 0 0rem var(--black);
                  }
               }
            }
         }
      }
   }
   .bottom {
      .text-end {
         text-align: right;
         .btn-custom {
            float: right;
         }
      }
   }
}
.rtl .cart-section .bottom .text-end {
   text-align: left !important;
   .btn-custom {
      float: left;
   }
}

// career section
