}

.form-check {
   label {
      cursor: pointer;
      color: var(--fontColor);
   }
   .form-check-input {
      border-radius: 5px;
      background-color: var(--white);
      border: 2px solid var(--borderColor);
      cursor: pointer;
      margin-top: 5px;
      &:checked {
         background-color: var(--primary);
         border-color: var(--primary);
      }
      &:focus {
         box-shadow: 0 0 0 0rem var(--black);
      }
   }
}
.rtl .form-check .form-check-input {
