// team section
.team-section {
   padding: 100px 0;
   .team-box {
      text-align: center;
      .img-box {
         width: 200px;
         height: 200px;
         border-radius: 200px;
         margin: auto;
         overflow: hidden;
         margin-bottom: 20px;
         img {
            object-fit: contain;
         }
      }
      .info {
