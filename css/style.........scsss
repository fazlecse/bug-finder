// navbar
.navbar {
   padding: 15px;
   transition: 0.2s;
   &.active {
      background: var(--white);
      box-shadow: var(--shadow2);
      .nav-item .nav-link {
         color: var(--black);
      }
   }
   .navbar-brand {
      img {
         width: 160px;
      }
   }
   .container {
      position: relative;
   }
   .navbar-nav {
      margin: auto;
   }
   .navbar-toggler {
      position: absolute;
      right: 15px;
      top: 0px;
      background: var(--primary);
      color: var(--white);
      width: 45px;
      height: 45px;
      border-radius: 10px;
      i {
         font-size: 16px;
         color: var(--white);
      }
      &:focus {
         box-shadow: 0 0 0 0rem;
      }
   }
   .close-btn {
      background: var(--primary);
      color: var(--white);
      width: 45px;
      height: 45px;
      border-radius: 10px;
   }
   .nav-item {
      margin: 0 15px;
      .nav-link {
         color: var(--white);
         text-transform: uppercase;
         font-size: 16px;
         font-weight: 400;
         padding: 10px 5px;
         position: relative;
         transition: 0.3s;
         &::before {
            content: "";
            position: absolute;
            background: var(--primary);
            left: -10px;
            bottom: 0;
            top: 0;
            height: 8px;
            width: 0px;
            border-radius: 100px;
            z-index: -1;
            margin: auto;
            opacity: 0;
            transition: 0.4s;
         }
         &.active,
         &.active::before,
         &:hover,
         &:hover::before {
            opacity: 1;
            color: var(--primary);
         }
      }
   }

   // background: var(--white);
   // box-shadow: var(--shadow2);
   .nav-item .nav-link {
      color: var(--black);
   }

   // navbar text
   .navbar-text {
      display: flex;
      .btn-custom {
         padding: 15px 35px;
         border-radius: 15px;
         text-transform: capitalize;
      }
   }
   .profile {
      position: relative;
      display: inline-block;
      cursor: pointer;
      border-radius: 100px;
      img {
         width: 40px;
         height: 40px;
         border-radius: 100px;
         border: 4px solid transparent;
         outline: 2px solid var(--bgLight2);
      }
   }
   // user panel
   .user-panel {
      position: relative;
      display: inline-block;
      width: auto;
      padding: 0;
      &:hover {
         .user-dropdown {
            opacity: 1;
            visibility: visible;
            top: 54px;
         }
      }
      .user-dropdown {
         background: var(--white);
         box-shadow: var(--shadow);
         width: 200px;
         overflow: hidden;
         padding-top: 0;
         padding-left: 0;
         position: absolute;
         right: 0;
         top: 64px;
         border-radius: 5px;
         visibility: hidden;
         transition: 0.2s;
         opacity: 0;
         z-index: 3;
         li {
            list-style: none;
            button {
               width: 190px;
               height: 38px;
               padding: 5px;
               margin: 5px;
               margin-bottom: 0;
               border-radius: 5px;
               font-size: 13px;
            }
            a {
               background: var(--white);
               color: var(--fontColor);
               font-weight: 400;
               padding: 5px;
               margin: 5px;
               border-radius: 5px;
               display: flex;
               transition: 0.4s;
               &:active,
               &:focus,
               &:hover {
                  background: var(--bgLight);
               }
               &:last-child {
                  border-bottom: none;
               }
               i {
                  width: 25px;
                  height: 25px;
                  border-radius: 5px;
                  color: var(--primary);
                  margin: 0 5px;
                  font-size: 16px;
                  text-align: center;
               }
            }
         }
      }
   }

   // messages
   .message {
      display: flex;
      position: relative;
      align-items: center;
      margin-bottom: 1px;
      padding: 10px 10px 10px 15px;
      padding-right: 5px;
      margin: 5px;
      border-radius: 5px;
      transition: 0.2s;
      &:last-child {
         margin-bottom: 0;
      }
      &:hover {
         background: var(--bgLight);
      }
      &.new {
         background: var(--bgLight);
         &::after {
            content: "";
            position: absolute;
            width: 8px;
            height: 8px;
            border-radius: 10px;
            background: var(--primary);
            top: 0;
            bottom: 0;
            right: 10px;
            margin: auto;
         }
      }
      &.active {
         .img-box {
            &::after {
               content: "";
               position: absolute;
               width: 12px;
               height: 12px;
               border-radius: 100%;
               background: var(--success);
               border: 2px solid var(--white);
               bottom: -3px;
               right: 5px;
               margin: auto;
            }
         }
      }
      .img-box {
         margin-right: 10px;
         position: relative;
         img {
            width: 40px;
            height: 40px;
            border-radius: 100px;
            object-fit: cover;
         }
      }
      .text-box {
         width: calc(100% - 75px);
         a {
            color: var(--fontColor);
            font-size: 13px;
            .name {
               color: var(--primary);
               font-weight: 400;
               font-size: 15px;
               .time {
                  // font-weight: 500;
                  text-align: right;
                  float: right;
                  color: var(--gray);
                  font-size: 12px;
               }
            }
            p {
               margin-bottom: 5px;
               text-overflow: ellipsis;
               display: block;
               overflow: hidden;
               white-space: nowrap;
            }
         }
      }
   }

   // notification panel
   .notification-panel {
      position: relative;
      display: inline-block;
      width: auto;
      padding: 0px 0;
      &:hover {
         .notification-dropdown {
            visibility: visible;
            opacity: 1;
            top: 54px;
         }
      }
      .dropdown-toggle {
         width: 40px;
         height: 40px;
         border-radius: 2px;
         position: relative;
         margin-right: 15px;
         padding-top: 4px;
         i {
            color: var(--primary);
            font-size: 24px;
         }
         &::after {
            display: none;
         }
         .count {
            background: var(--primary);
            min-width: 18px;
            height: 18px;
            border-radius: 50px;
            color: var(--white);
            position: absolute;
            top: 1px;
            right: 1px;
            font-size: 11px;
            font-weight: 500;
            padding-top: 1px;
         }
      }
      .dropdown-box {
         overflow-y: scroll;
         width: 100%;
         height: 100%;
         max-height: 300px;
         &::-webkit-scrollbar {
            width: 0;
            height: 100%;
         }
         &::-webkit-scrollbar-track {
            background: var(--black);
         }
         &::-webkit-scrollbar-thumb {
            background: var(--black);
            border-radius: 5px;
            &:hover {
               background: var(--primary);
            }
         }
      }
      .notification-dropdown {
         background: var(--white);
         box-shadow: var(--shadow);
         width: 290px;
         max-height: 340px;
         overflow: hidden;
         padding-bottom: 40px;
         padding-top: 0;
         padding-left: 0;
         position: absolute;
         right: -40px;
         top: 64px;
         border-radius: 5px;
         visibility: hidden;
         transition: 0.2s;
         opacity: 0;
         z-index: 10;
         li {
            padding: 5px;
            border-bottom: 1px solid var(--bgLight);
            a {
               padding: 10px 10px 10px 15px;
               border-radius: 5px;
               display: flex;
               color: var(--fontColor);
               white-space: normal;
               transition: 0.4s;
               &:active,
               &:focus,
               &:hover {
                  background: var(--bgLight);
               }
               i {
                  background: var(--primary);
                  border-radius: 5px;
                  margin-top: 5px;
                  padding: 4px;
                  width: 30px;
                  height: 30px;
                  text-align: center;
                  margin-right: 10px;
                  color: var(--white);
                  font-size: 16px;
               }
               .text {
                  width: calc(100% - 40px);
                  p {
                     font-size: 14px;
                     font-weight: 400;
                     margin-bottom: 5px;
                  }
                  .time {
                     font-size: 12px;
                     font-weight: 500;
                     color: var(--primary);
                  }
               }
            }
         }
         .clear-all {
            background: var(--white);
            font-weight: 500;
            font-size: 14px;
            text-transform: capitalize;
            border-top: 1px solid var(--bgLight);
            text-align: center;
            padding: 10px 10px 11px 15px;
            position: absolute;
            bottom: 0;
            left: 0;
            a {
               color: var(--primary);
               transition: 0.4s;
               &:hover {
                  color: var(--primary);
               }
            }
         }
      }
   }
   .notification-panel.inbox {
      .notification-dropdown {
         right: -95px;
      }
      .dropdown-toggle {
         margin-right: 0;
      }
   }
   .dropdown {
      cursor: pointer;
      .dropdown-menu {
         background: var(--white);
         box-shadow: var(--shadow);
         border-top: 2px solid var(--primary) !important;
         border: none;
         top: 100%;
         visibility: hidden;
         opacity: 0;
         display: block;
         transition: 0.3s;
         margin-top: 20px;
         padding: 5px;
         a {
            color: var(--fontColor);
            text-transform: capitalize;
            margin: 2px 0;
            padding: 6px 12px;
            border-radius: 4px;
            transition: 0.4s;
            &:hover {
               background: var(--bgLight);
               color: var(--primary);
            }
         }
         .dropdown-toggle::after {
            position: absolute;
            right: 15px;
            top: 50%;
            bottom: 50%;
            margin: auto;
         }
      }
      &:hover {
         .dropdown-menu {
            visibility: visible;
            opacity: 1;
            margin-bottom: 0;
            margin-top: 1px;
         }
      }
   }
}
.rtl .navbar {
   .navbar-nav {
      margin-right: auto;
      margin-left: auto;
   }
   .navbar-toggler {
      right: auto;
      left: 15px;
   }
   .navbar-brand {
      margin-right: 0;
   }
   .nav-item .nav-link::before {
      left: auto;
      right: -10px;
   }
   .notification-panel .dropdown-toggle {
      margin-right: 0;
      margin-left: 15px;
