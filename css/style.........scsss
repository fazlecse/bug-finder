      transition: 0.4s;
      z-index: 1;
      &:focus {
         z-index: 3;
         border-color: var(--borderColor);
         outline: 0;
         box-shadow: 0 0 0 0rem rgba(13, 110, 253, 0.25);
      }
      &::after {
         border-radius: 5px;
         background-image: url(https://bug-finder.s3.ap-southeast-1.amazonaws.com/assets/icon/plus.png);
         background-size: contain;
         position: absolute;
         right: 25px;
         height: 24px;
         width: 24px;
         background-position: center;
      }
      span {
         margin-right: 10px;
      }
   }
   .accordion-body {
      padding: 30px;
      border-radius: 5px;
   }
   .accordion-button:not(.collapsed) {
      box-shadow: none;
      color: var(--primary);
      // border-bottom: 1px solid var(--bgLight);
      &::after {
         background-image: url(https://bug-finder.s3.ap-southeast-1.amazonaws.com/assets/icon/minus.png);
      }
   }
