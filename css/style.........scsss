   // overflow-x: hidden;
   // background: var(--bgLight2);
   padding: 100px 0;
   position: relative;
   z-index: 1;
   overflow: hidden;
   background: var(--bgLight2);
   &::after {
      content: "";
      position: absolute;
      width: 100%;
      height: 200px;
      left: 0;
      top: 0;
      background: linear-gradient(180deg, var(--white) 0%, rgba(255, 255, 255, 0) 100%);
      z-index: 0;
   }
   &::before {
      content: "";
      position: absolute;
      width: 100%;
      height: 200px;
      left: 0;
      bottom: 0;
      background: linear-gradient(180deg, rgba(255, 255, 255, 0) 0%, var(--white) 100%);
      z-index: 0;
   }
   .nav-pills {
      width: 100%;
      .nav-link {
         background: var(--white);
         position: relative;
         // border-left: 2px solid var(--primary);
         color: var(--primary);
         width: 100%;
         margin-bottom: 15px;
         font-family: "Oswald", sans-serif;
         text-transform: uppercase;
         display: inline-block;
         padding: 15px;
         text-align: center;
         font-size: 16px;
         font-weight: 400;
         height: 60px;
         z-index: 1;
         border-radius: 5px;
         overflow: hidden;
         transition: 0.4s;
         &::before {
            content: "";
            position: absolute;
            left: 0;
            top: 0;
            width: 2px;
            height: 100%;
            background: var(--primary);
            border-radius: 5px;
            border-left: 2px solid var(--primary);
            z-index: -1;
            transition: 0.4s;
         }
         &:hover,
         &.active {
            color: var(--white);
            &::before {
               width: 100%;
            }
         }
      }
   }
   .accordion-item {
      background: var(--white);
      // box-shadow: var(--shadow);
      margin-bottom: 25px;
      border-radius: 10px;
      border: none;
   }
   .accordion-button {
      background: var(--white);
      border-bottom: 1px solid var(--white);
      border: none;
      border-radius: 10px !important;
      padding-left: 0;
      font-size: 20px;
      font-weight: 400;
      padding: 30px;
      padding-right: 70px;
      font-family: "Oswald", sans-serif;
      color: var(--black);
