// message wrapper
.message-wrapper {
   box-shadow: var(--shadow);
   border-radius: 10px;
   overflow: hidden;
   .messages-box {
      margin-bottom: 15px;
      max-height: 661px;
      background: var(--white);
      overflow-y: auto;
      &::-webkit-scrollbar {
         width: 3px;
         height: 100%;
      }
      &::-webkit-scrollbar-track {
         background: transparent;
      }
      &::-webkit-scrollbar-thumb {
         background: transparent;
         border-radius: 5px;
         transition: 0.4s;
         &:hover {
            background: var(--primary);
         }
      }
      &:hover::-webkit-scrollbar-thumb {
         background: var(--bgLight);
      }
      .message {
         display: flex;
         position: relative;
         align-items: center;
         margin-bottom: 1px;
         padding: 15px;
         padding-right: 5px;
         margin: 5px 3px 5px 5px;
         border-radius: 5px;
         transition: 0.4s;
         &:last-child {
            margin-bottom: 0;
         }
         &:hover {
            background: var(--bgLight);
         }
         &.new {
            background: var(--bgLight);
            &::after {
               content: "";
               position: absolute;
               width: 8px;
               height: 8px;
               border-radius: 10px;
               background: var(--primary);
               top: 0;
               bottom: 0;
               right: 10px;
               margin: auto;
            }
         }
         &.active {
            .img-box {
               &::after {
                  content: "";
                  position: absolute;
                  width: 12px;
                  height: 12px;
                  border-radius: 100%;
                  background: var(--success);
                  border: 2px solid var(--white);
                  bottom: -3px;
                  right: 5px;
                  margin: auto;
               }
            }
         }
         .img-box {
            margin-right: 10px;
            position: relative;
            img {
               width: 40px;
               height: 40px;
               border-radius: 100px;
               object-fit: cover;
            }
         }
         .text-box {
            width: calc(100% - 75px);
            a {
               color: var(--fontColor);
               font-size: 13px;
               .name {
                  color: var(--primary);
                  font-weight: 400;
                  font-size: 15px;
                  .time {
                     // font-weight: 500;
                     text-align: right;
                     float: right;
                     color: var(--gray);
                     font-size: 12px;
                  }
               }
               p {
                  margin-bottom: 5px;
                  text-overflow: ellipsis;
                  display: block;
                  overflow: hidden;
                  white-space: nowrap;
               }
            }
         }
      }
   }

   .inbox-wrapper {
      // border-left: 1px solid var(--borderColor);
      background: var(--white);
      position: relative;
      .top-bar {
         display: flex;
         align-items: center;
         justify-content: space-between;
         padding: 10px 15px;
         border-bottom: 1px solid var(--borderColor);
         .user {
            width: 50px;
            height: 50px;
            border-radius: 100px;
            margin-right: 10px;
            object-fit: cover;
         }
         .name {
            a {
               color: var(--fontColor);
               display: block;
            }
            span {
               color: var(--success);
               font-size: 14px;
            }
         }
         .info-btn {
            width: 40px;
            height: 40px;
            border-radius: 100px;
            background: var(--bgLight2);
            border: none;
            color: var(--primary);
            i {
               color: var(--primary);
            }
         }
      }
      .chats {
         padding: 30px 15px 10px 15px;
         height: 456px;
         overflow-y: auto;
         position: relative;
         &::-webkit-scrollbar {
            width: 3px;
            height: 100%;
         }
         &::-webkit-scrollbar-track {
            background: transparent;
         }
         &::-webkit-scrollbar-thumb {
            background: transparent;
            border-radius: 5px;
            transition: 0.4s;
            &:hover {
               background: var(--primary);
            }
         }
         &:hover::-webkit-scrollbar-thumb {
            background: var(--bgLight);
         }
         .chat-box {
            display: flex;
            justify-content: flex-end;
            margin-bottom: 20px;
            .img {
               margin-left: 10px;
               min-width: 30px;
               min-height: 30px;
               img {
                  width: 30px;
                  height: 30px;
                  border-radius: 30px;
                  object-fit: cover;
               }
            }
            .text-wrapper {
               .text {
                  padding: 10px;
                  background: var(--bgLight);
                  border-radius: 5px;
                  float: right;
                  margin-bottom: 5px;
                  max-width: 300px;
                  p {
                     margin-bottom: 0;
                     font-size: 14px;
                  }
               }
               .attachment-wrapper {
                  text-align: right;
               }
               .name {
                  text-align: right;
                  margin-bottom: 5px;
               }
               .time {
                  font-size: 12px;
                  display: inline-block;
                  width: 100%;
                  text-align: right;
               }
               .attachment {
                  display: inline-block;
                  img {
                     max-width: 100px;
                     border-radius: 3px;
                  }
               }
               .file {
                  color: var(--primary);
                  margin-bottom: 5px;
                  text-align: right;
                  a {
                     color: var(--primary);
                     display: inline-block;
                     margin-right: 5px;
                  }
               }
            }
         }
         .opposite-side {
            justify-content: flex-start;
            .img {
               margin-left: 0;
               margin-right: 10px;
            }
            .text-wrapper {
               .text {
                  float: left;
               }
               .time {
                  text-align: left;
               }
               .file {
                  text-align: left;
               }
               .name {
                  text-align: left;
               }
               .attachment-wrapper {
                  text-align: left;
               }
            }
         }
      }
      .img-preview {
         background: var(--bgLight);
         display: inline-block;
         position: relative;
         left: 0;
         bottom: 5px;
         padding: 5px;
         padding-bottom: 0;
         max-width: 100px;
         border-radius: 3px;
         img {
            width: 100%;
            height: auto;
            border-radius: 5px;
         }
         .img-info {
            padding: 0 5px;
            text-overflow: ellipsis;
            overflow: hidden;
            white-space: nowrap;
            span {
               font-size: 12px;
            }
            span.size {
               color: var(--gray);
               font-size: 10px;
            }
         }
         button.delete {
            position: absolute;
            right: -10px;
            top: -10px;
            background: var(--bgLight);
            width: 20px;
            height: 20px;
            border-radius: 20px;
            text-align: center;
            border: none;
            i {
               font-size: 14px;
               color: var(--secondary);
               padding-bottom: 3px;
            }
         }
      }
      .typing-area {
         background: var(--white);
         border-top: 1px solid var(--bgLight);
         padding: 20px;
         .input-group {
            border-radius: 5px;
            background: var(--bgLight);
            overflow: hidden;
            position: relative;
            .form-control {
               height: 45px;
               background: var(--bgLight);
               border-radius: 0;
               border: none;
               font-size: 13px;
               color: var(--gray);
               caret-color: var(--primary);
               padding: 7px;
               &:focus {
                  box-shadow: 0 0 0 0rem rgba(13, 110, 253, 0.25);
                  border: none;
               }
            }
            .send-file-btn {
               background: var(--bgLight);
               position: relative;
               border: none;
               width: 40px;
               height: 100%;
               border-top-left-radius: 3px;
               border-bottom-left-radius: 3px;
               text-align: center;
               color: var(--gray);
               i {
                  transition: 0.4s;
                  position: relative;
                  color: var(--primary);
               }
               &:hover {
                  i {
                     color: var(--primary);
                  }
               }
               .form-control {
                  position: absolute;
                  left: 0;
                  top: 0;
                  width: 100%;
                  height: 100%;
                  opacity: 0;
               }
            }
            .submit-btn {
               width: 38px;
               height: 38px;
               right: 4px;
               top: 4px;
               position: relative;
               border-radius: 5px !important;
               border: none;
               background: var(--primary);
               border: none;
               border-top-right-radius: 3px;
               border-bottom-right-radius: 3px;
               z-index: 3;
               i {
                  color: var(--white);
                  font-size: 16px;
                  transition: 0.4s;
               }
            }
         }
      }
   }
   .dot {
      display: inline-block;
      width: 6px;
      height: 6px;
      border-radius: 50%;
      background-color: var(--primary);
      position: relative;
      top: -12px;
   }
   .dots {
      height: 12px;
   }
   .dots .dot:nth-last-child(1) {
      animation: jumpingAnimation 0.6s 0.1s ease-in infinite;
   }
   .dots .dot:nth-last-child(2) {
      animation: jumpingAnimation 0.6s 0.2s ease-in infinite;
   }
   .dots .dot:nth-last-child(3) {
      animation: jumpingAnimation 0.6s 0.3s ease-in infinite;
   }

   @keyframes jumpingAnimation {
      0% {
         transform: translate3d(0, 0, 0);
      }
      50% {
         transform: translate3d(0, 8px, 0);
      }
      100% {
         transform: translate3d(0, 0, 0);
      }
   }
}

.rtl .message-wrapper {
   .inbox-wrapper {
      border-left: none;
      border-right: 1px solid var(--borderColor);
      .top-bar .user {
         margin-right: 0;
         margin-left: 10px;
      }
      .chats .chat-box {
         .img {
            margin-left: 0;
            margin-right: 10px;
         }
         .text-wrapper {
            .text {
               float: left;
            }
            .attachment-wrapper,
            .text,
            .time,
            .name,
            .file {
               text-align: left;
            }
         }
      }
      .chats .opposite-side {
         .img {
            margin-right: 0;
            margin-left: 10px;
         }
         .text-wrapper {
            .text {
               float: right;
            }
            .attachment-wrapper,
            .text,
            .time,
            .name,
            .file {
               text-align: right;
            }
         }
      }
      .typing-area .input-group .submit-btn {
         right: auto;
         left: 5px;
      }
      .img-preview button.delete {
         right: auto;
         left: -10px;
      }
   }

   .message {
      padding-right: 15px;
      padding-left: 5px;
      .img-box {
         margin-right: 0;
         margin-left: 10px;
         &::after {
            right: auto;
            left: 5px;
         }
      }
      .text-box a .name .time {
         float: left;
         text-align: left;
      }
      &.new::after {
         right: auto;
         left: 10px;
      }
      &.active .img-box::after {
         right: auto;
         left: 5px;
      }
   }
}

