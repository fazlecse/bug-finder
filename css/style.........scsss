:root {
   --primary: #5956e9;
   --primaryAlpha: rgba(88, 86, 233, 0.2);
   --secondary: #e279f9;
   --success: #26cc8c;
   --successAlpha: rgba(38, 204, 140, 0.2);
   --danger: #ff6379;
   --dangerAlpha: rgba(255, 99, 120, 0.2);
   --info: #59c8ff;
   --infoAlpha: rgba(89, 200, 255, 0.2);
   --warning: #ffce00;
   --warningAlpha: rgba(255, 204, 0, 0.2);
   --gold: #ffc107;
   --white: #fff;
   --black: #2f2d52;
   --bgLight: #e9f1ff;
   --bgLight2: #f4f8ff;
   --dark: #131022;
   --borderColor: #e5e5e5;
   --fontColor: #696969;
   --fontColor2: #a9a8b6;
   --yt: #f20000;
   --fb: #1877f2;
   --ig: #e2306c;
   --tw: #1d9bf0;
   --in: #0172b1;
   --google: #2ba24c;
   --shadow: 0 2px 20px 0 rgba(0, 0, 0, 0.08);
   --shadow: 0px 3px 10px 0px rgba(38, 59, 94, 0.1);
   --shadow2: 0 1px 2px rgb(0 0 0 / 20%);
}

* {
   margin: 0;
   padding: 0;
   box-sizing: border-box;
}
body {
   // font-family: "DM Sans", sans-serif;
   font-family: "PT Sans Narrow", sans-serif;
   color: var(--fontColor);
   background: var(--white);
   font-weight: 400;
   font-size: 16px;
}
body::-webkit-scrollbar {
   width: 10px;
   height: 100%;
}
body::-webkit-scrollbar-track {
   background: transparent;
}
body::-webkit-scrollbar-thumb {
   background: var(--primary);
   border-radius: 4px;
   transition: 0.4s;
   &:hover {
      background: var(--primary);
   }
}

.rtl {
   direction: rtl;
   overflow-x: hidden !important;
}
.rtl .select2-container--default .select2-selection--single .select2-selection__arrow {
   right: auto;
   left: 12px;
}
.rtl .select2-container--default .select2-selection--single .select2-selection__rendered {
